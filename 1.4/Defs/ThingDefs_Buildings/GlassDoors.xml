<Defs>

    <!--========================= Default Glass Door (1x1) =============================-->

    <ThingDef ParentName="SD_GlassDoorBase">
        <defName>SD_GlassDoorDefault</defName>
        <label>default glass door (1x1)</label>
        <thingClass>StevesDoors.Building_UnmirroredDoor</thingClass>
        <description>Divides rooms. Powered and opens quickly. Color-changeable glow via the color picker. Allows light through as well.</description>
        <uiIconPath>Things/Building/Door/SD_GlassDoorDefault_MenuIcon</uiIconPath>
        <building>
            <blueprintGraphicData>
                <texPath>Things/Building/Door/SD_GlassDoorDefault_Blueprint</texPath>
            </blueprintGraphicData>
        </building>
        <researchPrerequisites>
            <li>Autodoors</li>
            <li>ColoredLights</li>
        </researchPrerequisites>
        <costList>
            <Steel>10</Steel>
            <ComponentIndustrial>1</ComponentIndustrial>
        </costList>
        <graphicData>
            <texPath>Things/Building/Door/SD_Blank</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <damageData>
                <enabled>false</enabled>
            </damageData>
        </graphicData>
        <comps>
            <li Class="CompProperties_Forbiddable"/>
            <li Class="CompProperties_Power">
                <compClass>CompPowerTransmitter</compClass>
                <transmitsPower>true</transmitsPower>
            </li>
            <li Class="CompProperties_Glower">
                <glowRadius>3</glowRadius>
                <colorPickerEnabled>true</colorPickerEnabled>
            </li>
            <li Class="StevesDoors.CompProperties_EnhancedDoorGraphics">
                <isIrisDoor>false</isIrisDoor>
                <doorIrisMaxAngle>0</doorIrisMaxAngle>
                <defaultDoorLeftGraphic>
                    <texPath>Things/Building/Door/SD_GlassDoorDefaultLeft</texPath>
					<graphicClass>Graphic_Single</graphicClass>
                    <shaderType>Transparent</shaderType>
                    <drawSize>(1.0, 1.0)</drawSize>
                </defaultDoorLeftGraphic>
                <defaultDoorRightGraphic>
                    <texPath>Things/Building/Door/SD_GlassDoorDefaultRight</texPath>
					<graphicClass>Graphic_Single</graphicClass>
                    <shaderType>Transparent</shaderType>
                    <drawSize>(1.0, 1.0)</drawSize>
                </defaultDoorRightGraphic>
                <extraStaticDoorGraphics>
                    <li>
                        <texPath>Things/Building/Door/SD_GlassDoorDefaultLeftFrame</texPath>
					    <graphicClass>Graphic_Single</graphicClass>
                        <shaderType>Transparent</shaderType>
                        <drawSize>(1.0, 1.0)</drawSize>
                    </li>
                    <li>
                        <texPath>Things/Building/Door/SD_GlassDoorDefaultRightFrame</texPath>
					    <graphicClass>Graphic_Single</graphicClass>
                        <shaderType>Transparent</shaderType>
                        <drawSize>(1.0, 1.0)</drawSize>
                    </li>
                </extraStaticDoorGraphics>
            </li>
        </comps>
    </ThingDef>

